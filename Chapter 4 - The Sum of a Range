function range(start, end, step) {
    var array = [];
    if (step > 0) { 
        for (var i=start; i<=end; i+=step) {
        array.push(i);
        }
    } else {
        for (var j=start; j>=end; j+=step) {
        array.push(j);
        }
    }
    return array;
}

function sum(arrayOfNum) {
    var total = 0;
    for (var i=0; i<arrayOfNum.length; i++) {
        total += arrayOfNum[i];
    }
    return total;
}
console.log(sum(range(1,10)));
